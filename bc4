from pyvis.network import Network
import networkx as nx
import numpy as np
from matplotlib import pyplot as plt

G = nx.MultiDiGraph([(5,1),(4,2),(3,4)])

net = Network(width="1000px",height="700px",bgcolor="#222222",font_color="white")
net.from_nx(G)
net.show("od.png")

